{{#each this}}
<img src="{{flags.svg}}" alt="{{name.official}}" flag width="60px" height="30px">
<p class="country-info__name">{{name.official}}</p>
<p class="country-info__desc">Capital: {{capital}}</p>
<p class="country-info__desc">Population: {{population}}</p>
<p class="country-info__desc">Languages:
    {{#each languages}}
    <span> {{this}} </span>
    {{/each}}
</p>
{{/each}}


